<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="UtopiaIntegrationItem" parent="GtkListBoxRow">
    <property name="child">
      <object class="GtkBox">
        <property name="orientation">horizontal</property>
        <property name="hexpand">true</property>
        <child>
          <object class="GtkImage" id="icon">
            <property name="icon_size">large</property>
            <property name="margin_top">8</property>
            <property name="margin_bottom">8</property>
            <property name="margin_start">8</property>
            <property name="margin_end">8</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="margin_top">6</property>
            <property name="margin_start">8</property>
            <property name="margin_end">8</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkLabel" id="name">
                <property name="css_classes">ititle</property>
                <property name="xalign">0</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="uuid">
                <property name="css_classes">isubtitle</property>
                <property name="xalign">0</property>
                <property name="ellipsize">end</property>
                <property name="max_width_chars">42</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
